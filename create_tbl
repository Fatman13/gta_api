CREATE TABLE destinationServices (
	id INTEGER PRIMARY KEY,
	country TEXT,
	country_code TEXT,
	city TEXT,
	city_code TEXT,
	item_name TEXT,
	item_code TEXT,
	duration TEXT,
	language TEXT,
	dates_from TEXT,
	dates_to TEXT,
	days TEXT,
	min TEXT,
	conditions TEXT,
	pax_type TEXT,
	age_from TEXT,
	age_to TEXT,
	languageCode_list TEXT,
	net_price TEXT,
	closed_date TEXT,
	commentary TEXT,
	meeting_point TEXT
);